{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://dist.apache.org/repos/dist/release/incubator/shardingsphere/*"
    },
    {
      "pattern": "^https://github.com/apache/incubator-shardingsphere/*"
    },
    {
      "pattern": "^https://twitter.com*"
    }
  ], "replacementPatterns": [
  {
    "pattern": "^/cn/*",
    "replacement": "https://shardingsphere.apache.org/document/current/cn/"
  },
  {
    "pattern": "^/en/*",
    "replacement": "https://shardingsphere.apache.org/document/current/en/"
  }],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 10,
  "fallbackRetryDelay": "1000s",
  "aliveStatusCodes": [
    200,
    401,
    403
  ]
}
