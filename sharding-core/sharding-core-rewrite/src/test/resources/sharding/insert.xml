        <!--<input sql="INSERT INTO t_order SET user_id = 10, status = 'OK'" parameters="10, OK" />-->
        <!--<output sql="INSERT INTO t_order_1 SET user_id = 10, status = 'OK', order_id = 1" />-->
    <!--</rewrite-assertion>-->
</rewrite-assertions>
