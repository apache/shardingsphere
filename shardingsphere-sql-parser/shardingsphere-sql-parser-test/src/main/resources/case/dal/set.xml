<sql-parser-test-cases>
    <set-variable sql-case-id="set_parameter_equal" >
        <variable-assign value="'value'" >
            <variable name="configuration_parameter" />
        </variable-assign>
    </set-variable>
    <set-variable sql-case-id="set_parameter_equal_number" >
        <variable-assign value="3" >
            <variable name="extra_float_digits" />
        </variable-assign>
    </set-variable>
    <set-variable sql-case-id="set_parameter_equal_session_mysql" >
        <variable-assign value="'value'" >
            <variable name="configuration_parameter" scope="SESSION"/>
        </variable-assign>
    </set-variable>
    <set-variable sql-case-id="set_parameter_to" >
        <variable-assign value="'value'" >
            <variable name="configuration_parameter" />
        </variable-assign>
    </set-variable>
    <set-variable sql-case-id="set_parameter_for_session_scope" >
        <variable-assign value="'value'" >
            <variable name="configuration_parameter" scope="SESSION"/>
        </variable-assign>
    </set-variable>
    <set-variable sql-case-id="set_parameter_for_local_scope" >
        <variable-assign value="'value'" >
            <variable name="configuration_parameter" scope="LOCAL"/>
        </variable-assign>
    </set-variable>
    <set-variable sql-case-id="set_parameter_for_default_scope" >
        <variable-assign value="DEFAULT" >
            <variable name="configuration_parameter"/>
        </variable-assign>
    </set-variable>
    <set-variable sql-case-id="set_timezone_with_zero_value" />
    <set-variable sql-case-id="set_timezone_with_positive_value" />
    <set-variable sql-case-id="set_timezone_with_negative_value" />
    <set-variable sql-case-id="set_timezone_with_double_value" />
    <set-variable sql-case-id="set_timezone_for_default_scope" />
    <set-variable sql-case-id="set_timezone_for_local_scope" />
    <set-variable sql-case-id="set_timezone_for_session_scope" />
    <set-variable sql-case-id="set_timezone_for_local_scope_with_double_value" />
    <set-variable sql-case-id="set_parameter_equal_boolean" >
        <variable-assign value="true" >
            <variable name="extra_float_digits" />
        </variable-assign>
    </set-variable>
    <set-variable sql-case-id="set_parameter_equal_list" >
        <variable-assign value="1,2,3" >
            <variable name="extra_float_digits" />
        </variable-assign>
    </set-variable>
    <set-variable sql-case-id="set_parameter_equal_number_with_signal" >
        <variable-assign value="-10.5" >
            <variable name="extra_float_digits" />
        </variable-assign>
    </set-variable>
    <set-variable sql-case-id="set_names" >
        <variable-assign value="'utf8'" >
            <variable name="charset" />
        </variable-assign>
    </set-variable>
</sql-parser-test-cases>
